{
  "name": "porro",
  "version": "1.0.0",
  "description": "Fast leaky bucket algorithm implementation without timers",
  "type": "module",
  "main": "./porro.cjs",
  "exports": {
    "import": "./porro.mjs",
    "require": "./porro.cjs"
  },
  "types": "./porro.d.ts",
  "files": [
    "porro.d.ts",
    "porro.cjs",
    "porro.mjs"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "standard **/*.mjs",
    "test": "cross-env 'NODE_OPTIONS=--experimental-loader=@istanbuljs/esm-loader-hook' nyc ava",
    "prepublishOnly": "npm run lint && npm run build && npm run test"
  },
  "keywords": [
    "leaky",
    "bucket",
    "algorithm",
    "leek",
    "porro"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/esm-loader-hook": "0.1.2",
    "ava": "4.0.0-alpha.2",
    "cross-env": "^7.0.3",
    "nyc": "^15.1.0",
    "rollup": "^2.56.3",
    "standard": "^16.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/porro.git"
  }
}
